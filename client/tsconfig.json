{
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": false,
    "module": "ES6",
    "target": "ES5",
    "jsx": "react",
    "lib": ["DOM"],
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true,
    "moduleResolution": "node",
    "removeComments": true,
    "incremental": true,
    "paths": {
      "~/*": ["./js/*"]
    },
  }
}
